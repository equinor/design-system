{
  "name": "@equinor/eds-storybook-react",
  "version": "0.6.0",
  "engines": {
    "pnpm": ">=4",
    "node": ">=10.0.0"
  },
  "scripts": {
    "dev": "start-storybook -p 9000 --ci --no-dll",
    "build": "build-storybook --no-dll -c .storybook -o dist",
    "build:debug": "node --max_old_space_size=10000 --trace-deprecation ./node_modules/@storybook/react/bin/build.js --no-dll -c .storybook -o dist",
    "analyze:build": "webpack --profile --json > dist/stats.json",
    "analyze:serve": "webpack-bundle-analyzer dist/stats.json",
    "lint": "eslint stories/**/*.jsx"
  },
  "dependencies": {
    "@equinor/eds-core-react": "workspace:*",
    "@equinor/eds-icons": "workspace:*",
    "@equinor/eds-tokens": "workspace:*",
    "@storybook/react": "^6.0.27",
    "babel-runtime": "^6.26.0",
    "react-dom": "^16.13.1",
    "react-hook-form": "^5.7.2",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-transform-react-jsx": "^7.10.1",
    "@mdx-js/loader": "^1.6.19",
    "@mdx-js/react": "^1.6.21",
    "@storybook/addon-a11y": "^6.0.27",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-docs": "^6.0.27",
    "@storybook/addon-knobs": "^6.0.27",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storysource": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/source-loader": "^6.0.27",
    "@storybook/theming": "^6.0.27",
    "@types/react": "^16.9.54",
    "@types/react-dom": "^16.9.9",
    "@types/styled-components": "^5.1.4",
    "babel-loader": "^8.1.0",
    "core-js": "^3.8.0",
    "react": "^16.13.1",
    "react-is": "^16.13.1",
    "webpack-bundle-analyzer": "^4.1.0"
  }
}
