  "dependencies": {
    "@equinor/eds-icons": "workspace:*",
    "@equinor/eds-tokens": "workspace:*",
    "@testing-library/react-hooks": "^3.3.0",
    "focus-visible": "^5.1.0",
    "lodash.uniqueid": "^4.0.1"
  },
  "engines": {
    "pnpm": ">=4"
  }
