{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "lib": ["dom", "ES2022"],
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
