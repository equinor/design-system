{
  "compilerOptions": {
    "target": "ES2021",
    "module": "esnext",
    "lib": ["dom", "ES2021"],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "jsx": "react-jsx"
  }
}
