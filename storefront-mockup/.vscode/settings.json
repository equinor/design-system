{
    "css.validate": false,
    "stylelint.enable": true,
    "yaml.format.enable": true,
    "yaml.format.singleQuote": true,
    "yaml.format.bracketSpacing": true,
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4
    },
    "yaml.schemas": {
        "http://json.schemastore.org/stylelintrc": ".stylelintrc.yaml"
    }
}

