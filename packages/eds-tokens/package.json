{
  "name": "@equinor/eds-tokens",
  "version": "0.9.2",
  "description": "Design tokens for the Equinor Design System",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/tokens.cjs",
      "default": "./dist/tokens.cjs"
    },
    "./tokens.css": "./tokens.css",
    "./elements.css": "./elements.css",
    "./css/variables-static.css": "./build/css/variables-static.min.css",
    "./css/variables-static": "./build/css/variables-static.min.css",
    "./css/variables-dynamic.css": "./build/css/variables-dynamic.min.css",
    "./css/variables-dynamic": "./build/css/variables-dynamic.min.css",
    "./css/spacing/primitives.css": "./build/css/spacing/primitives-trimmed.css",
    "./css/spacing/comfortable.css": "./build/css/spacing/comfortable-verbose.css",
    "./css/spacing/spacious.css": "./build/css/spacing/spacious-verbose.css",
    "./json/color/flat/*": "./build/json/color/flat/*.json",
    "./json/spacing/flat/*": "./build/json/spacing/flat/*.json",
    "./json/color/nested/*": "./build/json/color/nested/*.json",
    "./json/spacing/nested/*": "./build/json/spacing/nested/*.json",
    "./js/color/*": "./build/js/color/*.js",
    "./js/spacing/*": "./build/js/spacing/*.js"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/tokens.cjs",
  "license": "MIT",
  "author": {
    "name": "EDS Core Team",
    "email": "fg_eds@equinor.com"
  },
  "homepage": "https://eds.equinor.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/equinor/design-system",
    "directory": "packages/eds-tokens"
  },
  "bugs": {
    "url": "https://github.com/equinor/design-system/issues"
  },
  "files": [
    "dist/*",
    "build/*",
    "commonjs/*",
    "tokens.css",
    "elements.css"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs && pnpm run types",
    "dev": "rollup -c -w",
    "types": "tsc",
    "prettier:check": "prettier --check src/",
    "_prebuild:generate-variables:build": "vite build --config vite.generate-variables.config.ts",
    "generate-variables": "pnpm run _prebuild:generate-variables:build && node ./build-generate-variables/generate-variables.js",
    "_prebuild:css-index": "copyfiles ./src/css/*.css ./build/css --flat",
    "_build:css:static": "lightningcss --bundle ./build/css/index-static.css -o ./build/css/variables-static.css",
    "_build:css:dynamic": "lightningcss --bundle ./build/css/index-dynamic.css -o ./build/css/variables-dynamic.css",
    "_build:css": "pnpm run _build:css:static && pnpm run _build:css:dynamic",
    "_build:css-min-dynamic": "lightningcss --minify --bundle ./build/css/variables-dynamic.css -o ./build/css/variables-dynamic.min.css",
    "_build:css-min-static": "lightningcss --minify --bundle ./build/css/variables-static.css -o ./build/css/variables-static.min.css",
    "_build:css-min": "pnpm run _build:css-min-static && pnpm run _build:css-min-dynamic",
    "build:css": "pnpm run _prebuild:css-index && pnpm run _build:css && pnpm run _build:css-min",
    "clean": "rimraf build",
    "build:variables": "pnpm run clean && pnpm run generate-variables && pnpm run build:css",
    "update-tokens:spacing-primitives": "sync-figma-to-tokens --file-key cpNchKjiIM19dPqTxE0fqg",
    "update-tokens:spacing-modes": "sync-figma-to-tokens --file-key FQQqyumcpPQoiFRCjdS9GM",
    "update-figma:spacing-primitives": "sync-tokens-to-figma --file-key cpNchKjiIM19dPqTxE0fqg",
    "update-figma:spacing-modes": "sync-tokens-to-figma --file-key FQQqyumcpPQoiFRCjdS9GM",
    "update-tokens:foundations": "sync-figma-to-tokens --file-key GnovDpL3UV6X51Ot7Kv6Im",
    "update-figma:foundations": "sync-tokens-to-figma --file-key GnovDpL3UV6X51Ot7Kv6Im",
    "update-tokens:color-static": "sync-figma-to-tokens --file-key OWxw2XogDLUt1aCvcDFXPw",
    "update-figma:color-static": "sync-tokens-to-figma --file-key OWxw2XogDLUt1aCvcDFXPw",
    "update-tokens:color-dynamic": "sync-figma-to-tokens --file-key nyPaQ3QnI1UAcxKW4a0d2c",
    "update-figma:color-dynamic": "sync-tokens-to-figma --file-key nyPaQ3QnI1UAcxKW4a0d2c",
    "update-tokens": "pnpm run update-tokens:foundations && pnpm run update-tokens:color-static && pnpm run update-tokens:color-dynamic && pnpm run update-tokens:spacing-primitives && pnpm run update-tokens:spacing-modes",
    "update-figma": "pnpm run update-figma:foundations && pnpm run update-figma:color-static && pnpm run update-figma:color-dynamic && pnpm run update-figma:spacing-primitives && pnpm run update-figma:spacing-modes"
  },
  "keywords": [
    "eds",
    "design system",
    "equinor",
    "design tokens"
  ],
  "devDependencies": {
    "@equinor/eds-tokens-build": "workspace:*",
    "@equinor/eds-tokens-sync": "workspace:*",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^22.18.0",
    "copyfiles": "^2.4.1",
    "lightningcss-cli": "^1.30.1",
    "prettier": "3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.0",
    "rollup-plugin-delete": "^2.2.0",
    "style-dictionary": "^5.0.3",
    "style-dictionary-utils": "^4.1.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.3"
  }
}
