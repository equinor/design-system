{
  "name": "@equinor/eds-tokens",
  "version": "0.10.0-beta.0",
  "description": "Design tokens for the Equinor Design System",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/tokens.cjs",
      "default": "./dist/tokens.cjs"
    },
    "./tokens.css": "./tokens.css",
    "./elements.css": "./elements.css",
    "./css/color/*": "./dist/css/color/*.css",
    "./css/spacing/*": "./dist/css/spacing/*.css",
    "./css/typography/*": "./dist/css/typography/*.css",
    "./json/color/flat/*": "./dist/json/color/flat/*.json",
    "./json/spacing/flat/*": "./dist/json/spacing/flat/*.json",
    "./json/typography/flat/*": "./dist/json/typography/flat/*.json",
    "./json/color/nested/*": "./dist/json/color/nested/*.json",
    "./json/spacing/nested/*": "./dist/json/spacing/nested/*.json",
    "./json/typography/nested/*": "./dist/json/typography/nested/*.json",
    "./ts/color/*": "./dist/ts/color/*.ts",
    "./ts/spacing/*": "./dist/ts/spacing/*.ts",
    "./ts/typography/*": "./dist/ts/typography/*.ts"
  },
  "types": "./dist/types/index.d.ts",
  "main": "./dist/tokens.cjs",
  "license": "MIT",
  "author": {
    "name": "EDS Core Team",
    "email": "fg_eds@equinor.com"
  },
  "homepage": "https://eds.equinor.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/equinor/design-system",
    "directory": "packages/eds-tokens"
  },
  "bugs": {
    "url": "https://github.com/equinor/design-system/issues"
  },
  "files": [
    "dist/*",
    "commonjs/*",
    "tokens.css",
    "elements.css"
  ],
  "scripts": {
    "build": "rollup -c --bundleConfigAsCjs && tsc && pnpm run build:beta",
    "dev": "rollup -c -w",
    "types": "tsc",
    "prettier:check": "prettier --check src/",
    "_sync:tokens": "ts-node --esm --project src/sync/tsconfig.json src/sync/scripts/sync_figma_to_tokens.ts",
    "update-tokens:primitives": "pnpm run _sync:tokens -- --file-key cVaqjfgt3gDiqbx10q3Pj8",
    "update-tokens:themes": "pnpm run _sync:tokens -- --file-key aRgKtCisnm98k9kVy6zasL",
    "update-tokens:typography-primitives": "pnpm run _sync:tokens -- --file-key cpNchKjiIM19dPqTxE0fqg",
    "update-tokens:typography-modes": "pnpm run _sync:tokens -- --file-key FQQqyumcpPQoiFRCjdS9GM",
    "_sync:figma": "ts-node --esm --project tsconfig.json src/sync/scripts/sync_tokens_to_figma.ts",
    "update-figma:primitives": "pnpm run _sync:figma -- --file-key cVaqjfgt3gDiqbx10q3Pj8",
    "update-figma:themes": "pnpm run _sync:figma -- --file-key aRgKtCisnm98k9kVy6zasL",
    "sync-figma-to-tokens": "pnpm run update-tokens:primitives && pnpm run update-tokens:themes",
    "sync-tokens-to-figma": "pnpm run update-figma:primitives && pnpm run update-figma:themes",
    "_prebuild:css-index": "shx cp src/index.css dist/css/index.css",
    "_build:css": "pnpm run _prebuild:css-index && lightningcss --bundle dist/css/index.css -o dist/css/variables.css",
    "_build:css-min": "pnpm run _prebuild:css-index && lightningcss --minify --bundle dist/css/index.css -o dist/css/variables.min.css",
    "clean:beta": "shx rm -rf dist/css dist/ts dist/json",
    "build:beta": "pnpm run clean:beta && ts-node --esm -P src/sync/tsconfig.json src/sync/build.ts && pnpm run _build:css && pnpm run _build:css-min",
    "test": "jest"
  },
  "keywords": [
    "eds",
    "design system",
    "equinor",
    "design tokens"
  ],
  "devDependencies": {
    "axios": "^1.6.0",
    "colorjs.io": "^0.5.0",
    "dotenv": "^16.3.1",
    "ts-node": "^10.9.1",
    "@figma/rest-api-spec": "^0.10.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.5",
    "jest": "^29.6.2",
    "lightningcss-cli": "^1.24.0",
    "prettier": "3.0.0",
    "rollup": "^4.2.0",
    "rollup-plugin-delete": "^2.0.0",
    "shx": "^0.3.4",
    "style-dictionary-utils": "^2.0.7",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "engines": {
    "pnpm": ">=8.15.4",
    "node": "^18.20.2"
  }
}
