    "react-hook-form": "^7.62.0",
    "react-router-dom": "^6.30.1",
    "rollup": "^4.50.0",
    "rollup-plugin-delete": "^2.2.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-preserve-directives": "^1.1.3",
    "storybook": "^9.1.5",
