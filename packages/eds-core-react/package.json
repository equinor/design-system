    "@floating-ui/react": "^0.27.16",
    "@internationalized/date": "^3.9.0",
    "@react-aria/utils": "^3.30.1",
    "@react-stately/calendar": "^3.8.4",
    "@react-stately/datepicker": "^3.15.1",
    "@react-types/shared": "^3.32.0",
    "@tanstack/react-virtual": "3.13.12",
