    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.5.2",
    "@types/jest": "^29.5.14",
    "@types/ramda": "^0.30.2",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
