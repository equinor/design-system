  },
  "peerDependencies": {
    "react": ">=16.8",
    "react-dom": ">=16.8",
    "styled-components": ">=4.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.8",
    "@equinor/eds-icons": "workspace:*",
    "@equinor/eds-tokens": "workspace:*",
    "@equinor/eds-utils": "workspace:*",
    "@popperjs/core": "2.9.2",
    "downshift": "^6.1.7",
    "react-fast-compare": "3.2.0",
    "react-popper": "2.2.5"
  },
  "engines": {
    "pnpm": ">=4",
    "node": ">=10.0.0"
