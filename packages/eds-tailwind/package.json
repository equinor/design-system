{
  "name": "@equinor/eds-tailwind",
  "version": "0.1.0",
  "private": false,
  "description": "Tailwind CSS theme for Equinor Design System (EDS)",
  "type": "module",
  "files": [
    "theme.css",
    "README.md"
  ],
  "style": "theme.css",
  "exports": {
    ".": {
      "style": "./theme.css",
      "import": "./theme.css",
      "default": "./theme.css"
    },
    "./theme.css": "./theme.css",
    "./theme": {
      "style": "./theme.css",
      "import": "./theme.css",
      "default": "./theme.css"
    }
  },
  "sideEffects": [
    "**/*.css"
  ],
  "peerDependencies": {
    "@equinor/eds-tokens": "workspace:^",
    "tailwindcss": ">=4.0.0 <5"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "tailwindcss": "^4.1.16",
    "vite": "^7.1.12"
  },
  "scripts": {
    "dev": "vite",
    "build:demo": "vite build",
    "preview": "vite preview --open dist/index.html"
  }
}
